{
    "name": "cruxinator/singletableinheritance",
    "description": "Store multiple, related, Laravel models in one table",
    "type": "library",
    "require": {
        "laravel/framework": "^5.6|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.0|^4.0|^5.0|^6.0|^7.0",
        "orchestra/database": "^3.0|^4.0|^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^8.5|^9.0",
        "ext-pdo": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cruxinator\\SingleTableInheritance\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cruxinator\\SingleTableInheritance\\Tests\\": "tests/",
            "Cruxinator\\SingleTableInheritance\\Tests\\Connections\\": "tests/connections/"
        }
    },
    "authors": [
        {
            "name": "Alex Goodwin",
            "email": "cyberia.resurrection@gmail.com"
        }
    ]
}
